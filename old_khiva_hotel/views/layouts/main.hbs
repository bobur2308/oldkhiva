	<link href="{{url}}css/animate.css" rel="stylesheet" type="text/css" media="all">
	<script src="{{url}}js/wow.min.js"></script>
	<script>
		new WOW().init();
	</script>
	<!-- animated-css -->
	<script src="{{url}}js/modernizr.js"></script>
	<script>
		$(document).ready(function(){
			if (Modernizr.touch) {
				// show the close overlay button
				$(".close-overlay").removeClass("hidden");
				// handle the adding of hover class when clicked
				$(".branch-gd").click(function(e){
					if (!$(this).hasClass("hover")) {
						$(this).addClass("hover");
					}
				});
				// handle the closing of the overlay
				$(".close-overlay").click(function(e){
					e.preventDefault();
					e.stopPropagation();
					if ($(this).closest(".branch-gd").hasClass("hover")) {
						$(this).closest(".branch-gd").removeClass("hover");
					}
				});
			} else {
				// handle the mouseenter functionality
				$(".branch-gd").mouseenter(function(){
					$(this).addClass("hover");
				})
				// handle the mouseleave functionality
				.mouseleave(function(){
					$(this).removeClass("hover");
				});
			}
		});
	</script>
	<script src="{{url}}js/bootstrap.min.js"></script>
</head>
